<?xml version="1.0" encoding="utf-8" ?>
<script classes="" title="Example Script" brand1="" script1="">
  <agent brand="Genius PPT" name="Test User" status="Demonstration">
  </agent>
  <panels number="10">
    <panel classes="col-xs-12" columns="2" name="SmallBranding" rows="1" title="" state="chromeless"></panel>
    <panel classes="col-xs-12" columns="1" name="Login" title="Log In" rows="5" state=""></panel>
    <panel classes="col-xs-12" columns="1" name="ScriptSelect" title="Script Select" rows = "4"></panel>
    <panel classes="col-xs-7" columns="2" name="MainInformation" title="Main Information" rows="8" state=""></panel>
    <panel classes="col-xs-5" columns="2" name="CallOutcomes" title="Call Outcomes" rows="1" state=""></panel>
    <panel classes="col-xs-5" columns="1" name="SalesOutcomes" title="Sales Outcome" rows="2" state=""></panel>
    <panel classes="col-xs-5" columns="3" name="NonSaleOutcomes" title="Non Sales Outcomes" row="5" state=""></panel>
